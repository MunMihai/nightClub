@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>@ViewBag.Title | nightClub</title>

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    @Styles.Render("~/bundles/aos/css")
    @Styles.Render("~/bundles/bootstrap/css")
    @Styles.Render("~/bundles/bootstrap_icons/css")
    @Styles.Render("~/bundles/boxicons/css")
    @Styles.Render("~/bundles/glightbox/css")
    @Styles.Render("~/bundles/remixicon/css")
    @Styles.Render("~/bundles/swiper_bundle/css")

    @if (IsSectionDefined("Styles"))
    {@RenderSection("styles", required: false)}


    <!-- Template Main CSS File -->
    @Styles.Render("~/bundles/main/css")
</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top ">
       @Html.Partial("_Header")
    </header><!-- End Header -->

<main id="main">
    @RenderBody()

</main><!-- End #main -->
    <!-- ======= Footer ======= -->
    <footer id="footer" >
    @Html.Partial("_Footer")
    </footer><!-- End Footer -->

    <div id="preloader"></div>
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

    <!-- Vendor JS Files -->
    @Scripts.Render("~/bundles/purecounter_vanilla/js")
    @Scripts.Render("~/bundles/aos/js")
    @Scripts.Render("~/bundles/glightbox/js")
    @Scripts.Render("~/bundles/isotope/js")
    @Scripts.Render("~/bundles/validate/js")


    <!-- Template Main JS File -->
    @Scripts.Render("~/bundles/main/js")

    @RenderSection("Scripts", required: false)

</body>

</html>


